[Unit]
Description=Jaidbot
After=network.target

[Service]
ExecStartPre=/usr/local/bin/installJaidbot
ExecStart=/usr/local/bin/jaidbot
User=jaid
Group=jaid
WorkingDirectory=/home/jaid/.config/Jaidbot
Restart=on-failure
RestartSec=60s
TimeoutStartSec=60s
PrivateTmp=true

[Install]
WantedBy=multi-user.target
